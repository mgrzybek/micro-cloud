      install:
        initImage:
          repository: "registry.ring0:5000/library/busybox"
          tag: "1.37.0"
          useDigest: false
        server:
          image:
            repository: "registry.ring0:5000/spiffe/spire-server"
            tag: "1.9.6"
            useDigest: false